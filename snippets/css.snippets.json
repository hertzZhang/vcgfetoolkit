{
  "fontsize lineheight": {
    "prefix": ["fl"],
    "body": ["font-size: ${1:16}px;", "line-height: ${2:24}px;"],
    "description": "font-size, line-height - FET"
  },
  "width height": {
    "prefix": ["wh"],
    "body": ["width: ${1:1280}px;", "height: ${2:344}px;"],
    "description": "width, height - FET"
  },
  "text ellipsis": {
    "prefix": ["te"],
    "body": [
      "overflow: hidden;",
      "white-space: nowrap;",
      "text-overflow: ellipsis;",
      "width: 100%;"
    ],
    "description": "单行文字溢出解决方案 - FET"
  },
  "text ellipsis WebKit": {
    "prefix": ["temw"],
    "body": [
      "overflow: hidden;",
      "display: -webkit-box;",
      "text-overflow: ellipsis;",
      "-webkit-line-clamp: ${1|2,3,4,5|};",
      "-webkit-box-orient: vertical;"
    ],
    "description": "WebKit内核浏览器文字多行溢出解决方案 - FET"
  },
  "horizontal vertical center": {
    "prefix": ["hvc"],
    "body": [
      "position: absolute;",
      "top: 50%;",
      "left: 50%;",
      "transform: translate(-50%, -50%);"
    ],
    "description": "元素横纵向居中显示 - FET"
  }
}
